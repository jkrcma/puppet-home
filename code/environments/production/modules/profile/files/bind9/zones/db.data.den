;
; data.den local TLD subdomain
;
$TTL	300
@	IN	SOA	ns.den. admin.den. (
			 2022012502		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
; nameserver NS
@	IN	NS	ns.den.

$ORIGIN data.den.

