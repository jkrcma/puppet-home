;
; .den local TLD reverse zone
;
$TTL	300
@	IN	SOA	ns1.den. admin.den. (
		     2022101901		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
; nameserver NS
@	IN	NS	ns1.den.
	IN	NS	ns2.den.

